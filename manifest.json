{
	"manifest_version": 3,
	"name": "ua changer",
	"version": "0.0.1",
	"description": "ua changer",
	"icons": {
		"32": "./images/icon.png"
	},
	"action": {
		"default_icon": {
			"32": "./images/icon.png"
		},
		"default_popup": "./popup.html"
	},
	"permissions": ["declarativeNetRequest"],
	"host_permissions": ["<all_urls>"],
	"background": {
		"service_worker": "./js/background.js"
	}
}
